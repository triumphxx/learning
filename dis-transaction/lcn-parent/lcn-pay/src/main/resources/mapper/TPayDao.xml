<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.triumphxx.lcnpay.dao.TPayDao">
    <resultMap id="BaseResultMap" type="com.triumphxx.lcnpay.bean.TPay">
        <result column="t_id" jdbcType="INTEGER" property="tId"/>
        <result column="t_name" jdbcType="VARCHAR" property="tName"/>
    </resultMap>
    <insert id="insert" parameterType="com.triumphxx.lcnpay.bean.TPay">

        <selectKey keyProperty="tId" keyColumn="tId" order="AFTER" resultType="java.lang.Integer">
            select last_insert_id()
        </selectKey>

        insert into t_pay (t_id, t_name)
        values (#{tId,jdbcType=INTEGER}, #{tName,jdbcType=VARCHAR})
    </insert>
    <insert id="insertSelective" parameterType="com.triumphxx.lcnpay.bean.TPay">
        insert into t_pay
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="tId != null">
                t_id,
            </if>
            <if test="tName != null">
                t_name,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="tId != null">
                #{tId,jdbcType=INTEGER},
            </if>
            <if test="tName != null">
                #{tName,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete from t_pay
        where t_id = #{tId,jdbcType=INTEGER}
    </delete>
</mapper>